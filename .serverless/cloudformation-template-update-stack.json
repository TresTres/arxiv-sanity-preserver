{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "The AWS CloudFormation template for this Serverless application",
  "Resources": {
    "ServerlessDeploymentBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketEncryption": {
          "ServerSideEncryptionConfiguration": [
            {
              "ServerSideEncryptionByDefault": {
                "SSEAlgorithm": "AES256"
              }
            }
          ]
        }
      }
    },
    "ServerlessDeploymentBucketPolicy": {
      "Type": "AWS::S3::BucketPolicy",
      "Properties": {
        "Bucket": {
          "Ref": "ServerlessDeploymentBucket"
        },
        "PolicyDocument": {
          "Statement": [
            {
              "Action": "s3:*",
              "Effect": "Deny",
              "Principal": "*",
              "Resource": [
                {
                  "Fn::Join": [
                    "",
                    [
                      "arn:",
                      {
                        "Ref": "AWS::Partition"
                      },
                      ":s3:::",
                      {
                        "Ref": "ServerlessDeploymentBucket"
                      },
                      "/*"
                    ]
                  ]
                }
              ],
              "Condition": {
                "Bool": {
                  "aws:SecureTransport": false
                }
              }
            }
          ]
        }
      }
    },
    "FetchUnderscorepapersLogGroup": {
      "Type": "AWS::Logs::LogGroup",
      "Properties": {
        "LogGroupName": "/aws/lambda/asp2-dev-fetch_papers"
      }
    },
    "DownloadUnderscorepdfsLogGroup": {
      "Type": "AWS::Logs::LogGroup",
      "Properties": {
        "LogGroupName": "/aws/lambda/asp2-dev-download_pdfs"
      }
    },
    "FetchUnderscorepapersLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "ServerlessDeploymentBucket"
          },
          "S3Key": "serverless/asp2/dev/1604739788669-2020-11-07T09:03:08.669Z/asp2.zip"
        },
        "FunctionName": "asp2-dev-fetch_papers",
        "Handler": "lambdas/fetch_papers.main",
        "MemorySize": 1024,
        "Role": "arn:aws:iam::555989344246:role/asp2_role",
        "Runtime": "python3.8",
        "Timeout": 6
      },
      "DependsOn": [
        "FetchUnderscorepapersLogGroup"
      ]
    },
    "FetchUnderscorepapersLambdaVersionvJGBh6AyERzWc0uoSNwTnKu5AieijXzKBH5DcXEISI": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "FetchUnderscorepapersLambdaFunction"
        },
        "CodeSha256": "ldbMMzGo8iA9h0TqGPHEusk1uGd1S66V+gYIVsqSvEs="
      }
    },
    "DownloadUnderscorepdfsLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "ServerlessDeploymentBucket"
          },
          "S3Key": "serverless/asp2/dev/1604739788669-2020-11-07T09:03:08.669Z/asp2.zip"
        },
        "FunctionName": "asp2-dev-download_pdfs",
        "Handler": "lambdas/download_pdfs.main",
        "MemorySize": 1024,
        "Role": "arn:aws:iam::555989344246:role/asp2_role",
        "Runtime": "python3.8",
        "Timeout": 6
      },
      "DependsOn": [
        "DownloadUnderscorepdfsLogGroup"
      ]
    },
    "DownloadUnderscorepdfsLambdaVersion3jieuZpq1Y1Y737bhTUMPR3T2Qzj2WIH6TcPVeTdLs": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "DownloadUnderscorepdfsLambdaFunction"
        },
        "CodeSha256": "ldbMMzGo8iA9h0TqGPHEusk1uGd1S66V+gYIVsqSvEs="
      }
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": {
        "Ref": "ServerlessDeploymentBucket"
      }
    },
    "FetchUnderscorepapersLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "FetchUnderscorepapersLambdaVersionvJGBh6AyERzWc0uoSNwTnKu5AieijXzKBH5DcXEISI"
      }
    },
    "DownloadUnderscorepdfsLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "DownloadUnderscorepdfsLambdaVersion3jieuZpq1Y1Y737bhTUMPR3T2Qzj2WIH6TcPVeTdLs"
      }
    }
  }
}