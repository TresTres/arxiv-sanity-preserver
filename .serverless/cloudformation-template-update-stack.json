              "Condition": {
                "ArnEquals": {
                  "aws:SourceArn": {
                    "Ref": "snsTopic"
                  }
                }
              }
            }
          ]
        },
        "Queues": [
          {
            "Ref": "deadLetterQueue"
          }
        ]
      }
    },
